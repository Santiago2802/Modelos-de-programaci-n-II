import numpy as np

def r(m,k):
     if k==3:
        return str([np.rot90(m,k)[0][1]])
    
     return str(np.rot90(m,k)[0][1:3])
        

def caracol (m):
    if m == []:
        return 0
    return str(m[0])+" "+ r(m,1)+" "+ r(m,2)+ " " + r(m,3)+" ['"+m[1][1]+"'] "
    

print(caracol([x.split() for x in open('C:/Users/asus/Desktop/matriz.txt').readlines()]))
